{
  "expo": {
    "name": "PachaQutec",
    "slug": "pachaqutec_mobile",
    "version": "1.0.0",
    "orientation": "portrait",
    "scheme": "pachaqutec",
    "userInterfaceStyle": "automatic",
    "android": {
      "package": "com.pachaqutec.app",
      "permissions": [
        "ACCESS_FINE_LOCATION",
        "ACCESS_COARSE_LOCATION"
      ]
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.pachaqutec.app"
    },
    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}